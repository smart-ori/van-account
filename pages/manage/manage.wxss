.manage{
  background-color: #f0f0f0;
  display: block;
}
.overview{
  position: fixed;
  top: 0;
  width: 100%;
  padding: 10rpx 20rpx;
  display: flex;
  align-items: center;
  font-size: 14px;
  background-image: radial-gradient(circle at left top,#1F8C31, #4ECC62);
  color: #fff;
}
.overview .man-total,.overview .woman-total{
  margin-left: 30rpx;
}
.content{
  height: calc(100vh - 160rpx);
  margin-top: 60rpx;
  background-color: #f8f8f8;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  overflow: auto;
}
.content .item{
  background-color: #fff;
  margin: 16rpx 0;
  font-size: 14px;
  color: #666;
  padding: 10rpx 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content .item .info{
  display: flex;
}
.content .item .edit-btn{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.content .item .edit-btn button{
  margin: 0 4rpx;
}
.weui-btn{
  margin-top: 0;
}
.add-user{
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #f8f8f8;
  padding: 20rpx 10rpx;
  z-index: 1;
  box-sizing: border-box;
}